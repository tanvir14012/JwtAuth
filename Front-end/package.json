{
  "name": "front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve --ssl true --ssl-key D:/E-Documents/SelfSignedSSL/code.key --ssl-cert D:/E-Documents/SelfSignedSSL/code.crt",
    "build": "ng build",
    "build:prod": "set NODE_OPTIONS=--openssl-legacy-provider && ng build -c production --output-path dist/wwwroot --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/file-input": "^6.0.0",
    "@angular/animations": "~12.0.5",
